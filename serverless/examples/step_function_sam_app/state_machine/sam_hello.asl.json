{
  "Comment": "A state machine that does mock stock trading.",
  "StartAt": "Hello Function",
  "States": {
    "Hello Function": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": "${HelloWorldFunctionArn}"
      },
      "End": true
    }
  }
}
